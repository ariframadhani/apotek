{
  "name": "apotek-baiturrahman",
  "version": "1.0.0",
  "description": "Apotek Baiturrahman from Hisyam's Client",
  "main": "index.js",
  "scripts": {
    "install-all": "yarn install && yarn install-client",
    "install-client": "cd client && yarn install",
    "start": "nodemon server.js",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "app": "cd client && serve -s build -l 5100",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "serve": "concurrently --kill-others-on-fail \"yarn server\" \"yarn app\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "author": "Arif Ramadhani",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "fakegoose": "^0.0.3",
    "faker": "^4.1.0",
    "global": "^4.3.2",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.1.7",
    "mongoose-dateonly": "^1.0.4",
    "mongoose-delete": "^0.4.0",
    "mongoose-deleted": "^1.5.0",
    "mongoose-id-validator": "^0.4.3",
    "mongoose-paginate": "^5.0.3",
    "mongoose-soft-remove": "^1.1.3",
    "mongoose-softdelete": "^0.6.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.4",
    "restify": "^7.2.1",
    "serve": "^10.0.0"
  },
  "devDependencies": {}
}
